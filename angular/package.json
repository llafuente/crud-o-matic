{
  "name": "crud-o-matic-front",
  "version": "1.0.0",
  "description": "",
  "main": "compiled/src/app/app/app.module.js",
  "scripts": {
    "****** CLEAN ******": "",
    "clean": "npm run clean:test && npm run clean:public",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "clean:public": "rimraf ./../src/main/resources/public/*",
    "clean:test": "rimraf coverage",
    "clean:node_modules": "rimraf node_modules",
    "****** TEST ******": "",
    "test": "",
    "test-disabled": "npm run clean:test && npm run test:karma && npm run test:coverage",
    "test:karma": "karma start ./karma/karma.config.js",
    "test:coverage": "npm run test:remap && npm run test:report",
    "test:remap": "remap-istanbul -i ./coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma",
    "test:report": "istanbul report",
    "****** BUILD ******": "",
    "build:dev": "npm run clean && npm run test && cross-env NODE_ENV=development webpack --progress --bail --profile",
    "build:production": "npm run clean && npm run test && cross-env NODE_ENV=production-jit webpack --progress --bail --profile",
    "build:production:aot": "npm run clean && npm run test && cross-env NODE_ENV=production-aot webpack --progress --bail --profile",
    "****** RUNNING ******": "",
    "start": "webpack-dev-server --inline --port 3003 --watch --content-base src/",
    "****** CUSTOM ******": "",
    "linthtml": "node ../html-formatter/bin/html-formatter.js src/bb4ui/**/*.html --write",
    "lintts": "prettier --write \"./src/**/*.ts\" && tslint --force --fix \"./src/**/*.ts\""
  },

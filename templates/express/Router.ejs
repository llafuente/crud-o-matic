import * as express from "express";
import { <%= createFunction %> } from "./<%= createFunction %>";
import { <%= readFunction %> } from "./<%= readFunction %>";
import { <%= updateFunction %> } from "./<%= updateFunction %>";
import { <%= listFunction %> } from "./<%= listFunction %>";
import { <%= deleteFunction %> } from "./<%= deleteFunction %>";

const <%= routerName %> = express.Router();
<%= routerName %>.post(
  '/<%= entitySingular %>',
  <%= createFunction %>,
  function (req: express.Request, res: express.Response, next: express.NextFunction) {
    res.status(201).json(req[<%= JSON.stringify(entitySingular) %>]);
  }
);
<%= routerName %>.get(
  '/<%= entitySingular %>',
  <%= listFunction %>,
  function (req: express.Request, res: express.Response, next: express.NextFunction) {
    res.status(200).json(req[<%= JSON.stringify(entityPlural) %>]);
  }
);
<%= routerName %>.get(
  '/<%= entitySingular %>/:<%= entityId %>',
  <%= readFunction %>,
  function (req: express.Request, res: express.Response, next: express.NextFunction) {
    res.status(200).json(req[<%= JSON.stringify(entitySingular) %>]);
  }
);
<%= routerName %>.patch(
  '/<%= entitySingular %>/:<%= entityId %>',
  <%= readFunction %>,
  <%= updateFunction %>,
  function (req: express.Request, res: express.Response, next: express.NextFunction) {
    res.status(200).json(req[<%= JSON.stringify(entitySingular) %>]);
  }
);
<%= routerName %>.delete(
  '/<%= entitySingular %>/:<%= entityId %>',
  <%= deleteFunction %>,
  function (req: express.Request, res: express.Response, next: express.NextFunction) {
    res.status(204).send();
  }
);

console.log("express create router <%= routerName %>");

export default <%= routerName %>;


{
  "name": "crud-o-matic",
  "version": "0.0.1",
  "description": "",
  "main": "generator/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-generator": "npm run build-generator && ava --bail --verbose ./generator-js/test/test.js",
    "test-generated": "npm run build-test && ava --bail --verbose ./test-js/test/user.test.js",
    "test": "npm run test-generator && npm run lint-generated-server && npm run lint-generated-client && npm run test-generated",
    "fast-test": "npm run test-generator && npm run test-generated",
    "pretty-generated-server": "prettier --write --parser typescript --print-width 120 \"generated/server/src/**/*.ts\"",
    "lint-generated-server": "npm run pretty-generated-server && tslint --fix -c ./tslint.json --type-check -p ./generated/server/tsconfig.json",
    "pretty-generated-client": "prettier --write --parser typescript --print-width 120 \"angular/src/**/*.ts\"",
    "lint-generated-client": "npm run pretty-generated-client &&  tslint --fix -c ./tslint.json angular/src/**/*.ts",
    "pretty": "prettier --write --parser typescript --print-width 120 \"generator/**/*.ts\"",
    "lint": "npm run pretty && tslint --fix -c ./tslint.json --type-check -p ./generator/tsconfig.json",
    "build-generator": "tsc -p ./generator/tsconfig.json",
    "build-test": "tsc -p ./test/tsconfig.json",
    "build": "npm run build-generator"
  },
  "author": "llafuente@noboxout.com",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "mongoose-timestamp": "^0.6.0",
    "jsontoxml": "0.0.11",
    "csv-write-stream": "^2.0.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "body-parser": "^1.17.2"
  },
  "dependencies": {
    "@types/express": "4.0.36",
    "@types/mongoose": "4.7.20",
    "@types/supertest": "2.0.2",
    "@types/node": "8.0.20",
    "ajv": "^5.2.2",
    "async": "^2.5.0",
    "bluebird": "^3.5.0",
    "ejs": "^2.5.7",
    "js-yaml": "^3.9.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.11.5",
    "mongoosemask": "0.0.6",
    "pluralize": "^6.0.0",
    "prettier": "1.5.3",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "winston": "^2.3.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "mongoose-timestamp": "^0.6.0",
    "jsontoxml": "0.0.11",
    "csv-write-stream": "^2.0.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "body-parser": "^1.17.2",
    "acorn": "^5.1.1",
    "ava": "^0.21.0",
    "babel-eslint": "^7.2.3",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.3.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1",
    "supertest": "^3.0.0",
    "tap": "^10.7.0"
  },
  "nyc": {
